<!DOCTYPE suite SYSTEM "cmdlinetester.dtd">
<suite id="unsupportedfips140-3.xml" timeout="1000">
	<test id="Ensure no fips.mode or fips.home set by default on unsupported platforms">
		<command>$EXE$ -XshowSettings -version</command>
		<output type="success" caseSensitive="no" regex="no">java version</output>
		<output type="failure" caseSensitive="no" regex="no">com.ibm.fips.mode</output>
		<output type="failure" caseSensitive="no" regex="no">com.ibm.fips.home</output>
	</test>
	<test id="Ensure -Xenablefips140-3 is not recognized on unsupported platforms">
		<command>$EXE$ -Xenablefips140-3 -XshowSettings -version</command>
		<output type="success" regex="no">Command-line option unrecognised</output>
		<output type="failure" caseSensitive="no" regex="no">com.ibm.fips.mode</output>
		<output type="failure" caseSensitive="no" regex="no">com.ibm.fips.home</output>
	</test>
	<test id="Ensure -Xdisablefips140-3 is not recognized on unsupported platforms">
		<command>$EXE$ -Xdisablefips140-3 -XshowSettings -version</command>
		<output type="success" regex="no">Command-line option unrecognised</output>
		<output type="failure" caseSensitive="no" regex="no">com.ibm.fips.mode</output>
		<output type="failure" caseSensitive="no" regex="no">com.ibm.fips.home</output>
	</test>
</suite>
